{
	"presets": [
		["@babel/env"],
	  	["@babel/preset-typescript", { "allExtensions": true }]
	],
	"plugins": [
		["@babel/plugin-transform-runtime", {
			"regenerator": true
		}],
		"@babel/plugin-syntax-dynamic-import",
		"@babel/proposal-class-properties",
		"@babel/proposal-object-rest-spread"
	]
}